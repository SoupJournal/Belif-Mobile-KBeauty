    "authors": [
        {
            "name": "Daniel Welsh",
            "email": "x@x.x"
        }
    ],
    "require": {
        "php": ">=5.2.0",
        "illuminate/support": "5.2.*",
        "laravelcollective/html": "5.2.*"
    },
    "autoload": {
        "classmap": [
